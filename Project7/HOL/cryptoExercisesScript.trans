   [oracles: DISK_THM] [axioms: ] []
|- !(s1 :string) (s2 :string). s1 <= s2 <=> (s1 = s2) \/ s1 < s2:
   thm
val string_lt_antisym =
   [oracles: DISK_THM] [axioms: ] []
|- !(s :string) (t :string). ~(s < t /\ t < s):
   thm
val string_lt_cases =
   [oracles: DISK_THM] [axioms: ] []
