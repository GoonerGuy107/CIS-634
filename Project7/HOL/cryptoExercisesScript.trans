val option_induction =
   [oracles: DISK_THM] [axioms: ] []
|- !(P :'a option -> bool).
     P (NONE :'a option) /\ (!(a :'a). P (SOME a)) ==>
     !(x :'a option). P x:
   thm
val option_nchotomy =
   [oracles: DISK_THM] [axioms: ] []
|- !(opt :'a option).
     (opt = (NONE :'a option)) \/ ?(x :'a). opt = SOME x:
   thm
val some_EQ =
   [oracles: DISK_THM] [axioms: ] []
|- ((some(x :'a). x = (y :'a)) = SOME y) /\
   ((some(x :'a). y = x) = SOME y):
   thm
val some_F =
