val string_lt_def =
   [oracles: DISK_THM] [axioms: ] []
|- (!(s :string). s < ("" :string) <=> F) /\
   (!(s :string) (c :char). ("" :string) < STRING c s <=> T) /\
   !(s2 :string) (s1 :string) (c2 :char) (c1 :char).
     STRING c1 s1 < STRING c2 s2 <=> c1 < c2 \/ (c1 = c2) /\ s1 < s2:
   thm
val string_lt_ind =
   [oracles: DISK_THM] [axioms: ] []
|- !(P :string -> string -> bool).
     (!(s :string). P s ("" :string)) /\
